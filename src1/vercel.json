{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/auth/spotify",
      "dest": "/api/auth/spotify.js"
      },
      {
        "src": "/api/auth/google",
      "dest": "/api/auth/google.js"
      },
      {
        "src": "/request/get-playlist",
      "dest": "/request/get-playlist"
      },
      {
        "src": "/request/Migrate",
      "dest": "/request/Migrate"
      }
    ]
  }
  